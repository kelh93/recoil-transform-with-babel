{
  "presets": [
    [
      "@babel/preset-env", 
      {
        "useBuiltIns": "entry",
        "corejs": {
          "version": "3",
          "proposals": true
        },
        "targets": {
          "chrome": 75,
          "ie": 10
        }
      }
    ],
    ["@babel/preset-react"]
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime", 
      {
        "corejs": false,
        "proposals": true
      }
    ]
  ]
}